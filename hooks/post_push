#!/bin/bash

TAGS=$(curl https://registry.hub.docker.com/v1/repositories/jjmerelo/alpine-raku/tags | perl -ne 'my @tags=/\"(2[^"]+)\"/g; print join(" ",@tags)')
which jq

for i in TAGS
do
    docker build hooks -build-arg CODE_VERSION=${i} -t jjmerelo/raku-test:${i}
    docker push jjmerelo/raku-test:${i}
done
